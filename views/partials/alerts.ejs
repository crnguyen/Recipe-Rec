<% if(alerts.error) { %>
    <% console.log(alerts.error) %> 
    <% alerts.error.forEach(msg => { %>
        <div class= "alert alert-error">
            <%= msg %>
        </div>
    <% }) %>
<% } %> 

<% if(alerts.success) { %>
    <% console.log(alerts.success) %> 
    <% alerts.success.forEach(msg => { %>
        <div class= "alert alert-success">
            <%= msg %>
        </div>
    <% }) %>
<% } %> 
